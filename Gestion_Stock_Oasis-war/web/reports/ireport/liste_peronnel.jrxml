     "public"."magasin" magasin INNER JOIN "public"."personnel" personnel ON magasin."idmagasin" = personnel."idmagasin"
     INNER JOIN "public"."qualite" qualite ON personnel."idqualite" = qualite."idqualite"
ORDER BY
     magasin.nom ASC]]>
